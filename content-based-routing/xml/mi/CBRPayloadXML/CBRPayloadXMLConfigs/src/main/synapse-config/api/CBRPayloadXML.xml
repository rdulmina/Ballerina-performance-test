<?xml version="1.0" encoding="UTF-8"?>
<api context="/CBRPayloadXML" name="CBRPayloadXML" xmlns="http://ws.apache.org/ns/synapse">
    <resource methods="POST">
        <inSequence>
            <filter regex=".*" source="//order[1]/symbol" xmlns:m="http://services.samples/xsd">
                <then>
                    <log>
                        <property name="name" value="dulimina"/>
                    </log>
                    <call>
                        <endpoint key="CatInfoEndPoint"/>
                    </call>
                </then>
                <else>
                    <payloadFactory description="Create response payload for unsupported operation" media-type="xml">
                        <format>
                            <Message xmlns="">Cannot find symbol</Message>
                        </format>
                        <args/>
                    </payloadFactory>
                </else>
            </filter>
            <respond/>
        </inSequence>
        <outSequence/>
        <faultSequence/>
    </resource>
</api>
